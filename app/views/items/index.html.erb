<%= render 'search' %>

<section class="info">
  <div class="sell">
    <h2><%= t(:sell) %></h2>
    <ul>
      <% t(:sell_info).each do |info| %>
        <li><%= info %></li>
      <% end %>
    </ul>
  </div>
  <div class="buy">
    <h2><%= t(:buy) %></h2>
    <ul>
      <% t(:buy_info).each do |info| %>
        <li><%= info %></li>
      <% end %>
    </ul>
  </div>
</section>
